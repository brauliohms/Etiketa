{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "tsx watch src/main.ts",
    "start": "cross-env PORT=8080 node dist/src/main.js",
    "copy:swagger": "mkdir -p dist/src/infra/configs/swagger && cp src/infra/configs/swagger/api.json dist/src/infra/configs/swagger",
    "build": "tsc && npm run copy:swagger",
    "migrations:make": "knex migrate:make -x ts --knexfile knexfile.ts",
    "migrations:latest": "knex migrate:latest --knexfile knexfile.ts",
    "watch-tsc": "npx tsc -w"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/knex": "^0.16.1",
    "@types/node": "^20.14.2",
    "@types/pg": "^8.11.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.12.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "ts-node": "^10.9.2",
    "tsx": "^4.14.0",
    "typescript": "^5.4.5"
  }
}
